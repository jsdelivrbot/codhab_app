{
  "name": "appcodhab",
  "version": "1.1.1",
  "description": "appcodhab: An Ionic project",
  "dependencies": {
    "compat": "^1.0.4",
    "console": "^0.5.2",
    "cordova-android": "6.3.0",
    "cordova-plugin-actionsheet": "^2.3.3",
    "cordova-plugin-camera": "^2.4.1",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-crosswalk-webview": "^2.4.0",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-geolocation": "^2.4.3",
    "cordova-plugin-inappbrowser": "^1.7.2",
    "cordova-plugin-mauron85-background-geolocation": "^2.3.3",
    "cordova-plugin-network-information": "^1.3.4",
    "cordova-plugin-splashscreen": "^3.2.2",
    "cordova-plugin-statusbar": "^2.3.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "device": "^0.3.8",
    "gulp": "^3.5.6",
    "ionic-plugin-keyboard": "^2.2.1",
    "onesignal-cordova-plugin": "^2.2.4",
    "phonegap-plugin-barcodescanner": "^6.0.8",
    "uk.co.workingedge.phonegap.plugin.launchnavigator": "^3.2.2"
  },
  "devDependencies": {
    "@ionic/cli-plugin-cordova": "1.6.2",
    "@ionic/cli-plugin-gulp": "1.1.1",
    "@ionic/cli-plugin-ionic1": "2.1.1",
    "bower": "^1.8.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-minify-css": "^0.3.13",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-util": "^3.0.7",
    "shelljs": "^0.7.8"
  },
  "cordovaPlugins": [
    {
      "locator": "https://github.com/phonegap/phonegap-plugin-barcodescanner.git",
      "id": "phonegap-plugin-barcodescanner"
    },
    "cordova-plugin-compat",
    "cordova-plugin-geolocation",
    "cordova-plugin-camera",
    "ionic-plugin-keyboard",
    "cordova-plugin-statusbar",
    "cordova-plugin-splashscreen",
    "cordova-plugin-whitelist",
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-inappbrowser",
    "cordova-plugin-mauron85-background-geolocation",
    "onesignal-cordova-plugin",
    "cordova-plugin-network-information",
    "cordova-plugin-crosswalk-webview"
  ],
  "cordovaPlatforms": [
    "android",
    {
      "platform": "android",
      "version": "",
      "locator": "android"
    }
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-mauron85-background-geolocation": {
        "ALWAYS_USAGE_DESCRIPTION": "Este aplicativo utiliza GPS, acione para uma melhor experiencia.",
        "ACCOUNT_TYPE": "$PACKAGE_NAME.account",
        "CONTENT_AUTHORITY": "$PACKAGE_NAME",
        "GOOGLE_PLAY_SERVICES_VERSION": "+",
        "ICON": "@mipmap/icon",
        "SMALL_ICON": "@mipmap/icon",
        "ACCOUNT_NAME": "@string/app_name",
        "ACCOUNT_LABEL": "@string/app_name"
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-actionsheet": {},
      "cordova-plugin-camera": {},
      "cordova-plugin-compat": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-network-information": {},
      "onesignal-cordova-plugin": {},
      "phonegap-plugin-barcodescanner": {},
      "uk.co.workingedge.phonegap.plugin.launchnavigator": {},
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      }
    },
    "platforms": [
      "android"
    ]
  }
}